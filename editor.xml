<application>
  <component name="DefaultFont">
    <option name="FONT_FAMILY" value="Droid Sans Mono" />
    <option name="FONT_REGULAR_SUB_FAMILY" value="Regular" />
    <option name="FONT_BOLD_SUB_FAMILY" value="Regular" />
    <option name="LINE_SPACING" value="1.01" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
    <option name="SOFT_WRAP_FILE_MASKS" value="*.md; *.txt; *.rst; *.adoc" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
  </component>
  <component name="JetEditorOptions">
    <option name="donTShowConversionDialog" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
  </component>
</application>